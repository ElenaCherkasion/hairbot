{
  "name": "hairbot",
  "version": "1.0.0",
  "description": "Telegram –±–æ—Ç –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞ —Ñ–æ—Ä–º—ã –ª–∏—Ü–∞ –∏ –ø–æ–¥–±–æ—Ä–∞ —Å—Ç—Ä–∏–∂–µ–∫ —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º OpenAI",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon src/index.js",
    "dev:watch": "nodemon --watch src --ext js,json --exec npm run dev",
    "dev:debug": "NODE_ENV=development nodemon --inspect src/index.js",
    "dev:with-db": "npm run db:create && npm run db:seed && npm run dev",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "npm install && npm run db:create && npm run db:seed",
    "setup:prod": "npm ci --only=production && npm run db:create",
    "reset": "npm run clean && npm install && npm run setup",
    "reset:hard": "npm run clean && rm -f package-lock.json && npm install && npm run setup",
    "db:create": "node scripts/create-tables.js",
    "db:seed": "node scripts/seed-database.js",
    "db:backup": "node scripts/backup-db.js",
    "db:backup:list": "node scripts/backup-db.js --list",
    "db:backup:restore": "node scripts/backup-db.js --restore",
    "db:migrate": "node scripts/migrate-db.js",
    "db:cleanup": "node scripts/cleanup-db.js",
    "db:stats": "node scripts/show-stats.js --database",
    "webhook:setup": "node scripts/setup-webhook.js",
    "webhook:info": "node scripts/setup-webhook.js info",
    "webhook:delete": "node scripts/setup-webhook.js delete",
    "webhook:test": "node scripts/setup-webhook.js test",
    "health": "node scripts/check-health.js",
    "health:detailed": "node scripts/check-health.js --detailed",
    "clean": "rm -rf node_modules logs backups",
    "clean:logs": "rm -f logs/*.log && echo '‚úÖ –õ–æ–≥–∏ –æ—á–∏—â–µ–Ω—ã'",
    "clean:backups": "rm -f backups/*.sql backups/*.db && echo '‚úÖ –ë—ç–∫–∞–ø—ã –æ—á–∏—â–µ–Ω—ã'",
    "clean:modules": "rm -rf node_modules && echo '‚úÖ Node modules —É–¥–∞–ª–µ–Ω—ã'",
    "clean:cache": "rm -rf .npm && echo '‚úÖ –ö—ç—à npm –æ—á–∏—â–µ–Ω'",
    "deploy": "npm run check:ci && npm start",
    "deploy:prod": "NODE_ENV=production npm run deploy",
    "docker:build": "docker build -t hairbot .",
    "docker:run": "docker run -p 3000:3000 --env-file .env hairbot",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app --env-file .env hairbot",
    "docker:compose": "docker-compose up",
    "docker:compose:build": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:restart": "docker-compose restart",
    "check-env": "node scripts/check-env.js",
    "check-env:create": "node scripts/check-env.js --create",
    "check-env:strict": "node scripts/check-env.js --strict",
    "check-env:ci": "node scripts/check-env-ci.js",
    "logs": "npm run logs:view",
    "logs:view": "tail -f logs/*.log 2>/dev/null || echo '‚ö†Ô∏è  –§–∞–π–ª—ã –ª–æ–≥–æ–≤ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã'",
    "logs:error": "tail -f logs/error.log 2>/dev/null || echo '‚ö†Ô∏è  –§–∞–π–ª error.log –Ω–µ –Ω–∞–π–¥–µ–Ω'",
    "logs:app": "tail -f logs/app.log 2>/dev/null || echo '‚ö†Ô∏è  –§–∞–π–ª app.log –Ω–µ –Ω–∞–π–¥–µ–Ω'",
    "logs:clear": "npm run clean:logs",
    "stats": "node scripts/show-stats.js",
    "stats:detailed": "node scripts/show-stats.js --detailed",
    "stats:system": "node scripts/show-stats.js --system",
    "stats:memory": "node scripts/show-stats.js --memory",
    "check:all": "npm run check-env && npm run health && npm run stats",
    "check:quick": "npm run check-env:ci && echo '‚úÖ –ë–∞–∑–æ–≤–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–æ–π–¥–µ–Ω–∞'",
    "check:ci": "npm run lint && npm test && npm run check-env:ci",
    "backup:all": "npm run db:backup && cp .env backups/.env.backup 2>/dev/null || echo '‚ö†Ô∏è  .env –Ω–µ –Ω–∞–π–¥–µ–Ω' && echo '‚úÖ –ë—ç–∫–∞–ø —Å–æ–∑–¥–∞–Ω'",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:force": "npm audit fix --force",
    "prod:setup": "npm ci --only=production && npm run check-env:ci",
    "prod:start": "NODE_ENV=production node index.js",
    "prod:with-backup": "npm run db:backup && npm run prod:start",
    "update": "npm update",
    "update:all": "npx npm-check-updates -u && npm install",
    "postinstall": "echo '‚úÖ –ó–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã'",
    "prestart": "echo 'üöÄ –ó–∞–ø—É—Å–∫ HairBot...'",
    "predeploy": "npm run check:ci",
    "version": "echo '–í–µ—Ä—Å–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∞'",
    "postversion": "git push && git push --tags"
  },
